# Kohler Anthem API Credentials
# Copy this file to .env and fill in your values
#
# Usage: source .env

# Azure AD B2C Authentication (extracted from APK)
export KOHLER_CLIENT_ID=your-client-id
export KOHLER_API_RESOURCE=your-api-resource-id

# User credentials
export KOHLER_USERNAME=your_email@example.com
export KOHLER_PASSWORD=your_password

# Azure APIM Subscription Key (captured via mitmproxy)
export KOHLER_APIM_KEY=your-apim-subscription-key

# Device Info (obtained from device discovery API after first auth)
export KOHLER_DEVICE_ID=gcs-xxxxxxxxx
export KOHLER_TENANT_ID=your-tenant-id

# Azure IoT Hub (optional, for MQTT status - captured via Frida)
export KOHLER_IOT_HUB_HOST=prd-hub.azure-devices.net
export KOHLER_IOT_DEVICE_ID=Android_your-customer-id_unique-suffix
export KOHLER_IOT_SHARED_ACCESS_KEY=your-shared-access-key
